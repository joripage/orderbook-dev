  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch App",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/oms/main.go",
      "envFile": "${workspaceFolder}/build/.base.env",
      "args": [
        "-config-file",
        "${workspaceFolder}/build/config.yaml"
      ]
    },
    {
      "name": "Launch Worker",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/worker/main.go",
      "envFile": "${workspaceFolder}/build/.base.env",
      "args": [
        "-config-file",
        "${workspaceFolder}/build/config.yaml"
      ]
    },
    {
      "name": "Launch Benchmark",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/benchmark/main.go",
      "args": []
    }
  ]
}